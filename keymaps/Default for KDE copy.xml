<keymap version="1" name="Default for KDE copy" parent="Default for KDE">
  <action id="$Copy">
    <keyboard-shortcut first-keystroke="ctrl c" />
    <keyboard-shortcut first-keystroke="ctrl insert" />
    <keyboard-shortcut first-keystroke="shift ctrl c" />
  </action>
  <action id="Back">
    <keyboard-shortcut first-keystroke="ctrl alt left" />
    <mouse-shortcut keystroke="button4" />
    <keyboard-shortcut first-keystroke="alt z" />
  </action>
  <action id="Clean">
    <keyboard-shortcut first-keystroke="shift ctrl 9" />
  </action>
  <action id="CopyPaths" />
  <action id="Editor Copy">
    <keyboard-shortcut first-keystroke="shift ctrl c" />
  </action>
  <action id="Forward">
    <keyboard-shortcut first-keystroke="ctrl alt right" />
    <mouse-shortcut keystroke="button5" />
    <keyboard-shortcut first-keystroke="alt x" />
  </action>
  <action id="IntroduceField" />
  <action id="ReformatCode">
    <keyboard-shortcut first-keystroke="shift alt l" />
    <keyboard-shortcut first-keystroke="ctrl alt f" />
  </action>
</keymap>